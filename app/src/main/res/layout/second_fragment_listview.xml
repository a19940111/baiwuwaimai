<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginTop="10dp">

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:scaleType="centerInside"
            android:id="@+id/imageButton3"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:background="@color/white"
            android:src="@drawable/mypage_list_icon_location"/>
        <ImageButton
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:scaleType="centerInside"
            android:id="@+id/imageButton4"
            android:background="@color/white"
            android:layout_marginTop="5dp"
            android:src="@drawable/mypage_list_icon_daijinjuan"
            android:layout_below="@+id/imageButton3"/>
        <ImageButton
            android:layout_marginTop="5dp"
            android:layout_below="@+id/imageButton4"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:scaleType="centerInside"
            android:id="@+id/imageButton5"
            android:background="@color/white"
            android:src="@drawable/mypage_list_icon_refund"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="我的送餐地址"
            android:textColor="@color/black"
            android:id="@+id/textView3"
            android:layout_alignParentTop="true"
            android:layout_alignBottom="@+id/imageButton3"
            android:gravity="center"
            android:layout_toRightOf="@+id/imageButton3"
            android:layout_alignTop="@+id/imageButton3"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="我的代金券"
            android:textColor="@color/black"
            android:id="@+id/textView4"
            android:layout_alignBottom="@+id/imageButton4"
            android:gravity="center"
            android:layout_toRightOf="@+id/imageButton4"
            android:layout_alignTop="@+id/imageButton4"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="我的退款"
            android:textColor="@color/black"
            android:id="@+id/textView5"
            android:layout_alignBottom="@+id/imageButton5"
            android:gravity="center"
            android:layout_alignTop="@+id/imageButton5"
            android:layout_toRightOf="@+id/imageButton5" />

    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginTop="20dp"
        >

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:scaleType="centerInside"
            android:id="@+id/imageButton6"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:background="@color/white"
            android:src="@drawable/my_messages"/>
        <ImageButton
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:scaleType="centerInside"
            android:id="@+id/imageButton7"
            android:background="@color/white"
            android:layout_marginTop="5dp"
            android:src="@drawable/mypage_list_icon_star"
            android:layout_below="@+id/imageButton6"/>
        <ImageButton
            android:layout_marginTop="5dp"
            android:layout_below="@+id/imageButton7"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:scaleType="centerInside"
            android:id="@+id/imageButton8"
            android:background="@color/white"
            android:src="@drawable/mypage_list_icon_comment"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="我的消息"
            android:textColor="@color/black"
            android:id="@+id/textView6"
            android:layout_alignParentTop="true"
            android:layout_alignBottom="@+id/imageButton6"
            android:gravity="center"
            android:layout_toRightOf="@+id/imageButton6"
            android:layout_alignTop="@+id/imageButton6"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="我的收藏"
            android:textColor="@color/black"
            android:id="@+id/textView7"
            android:layout_alignBottom="@+id/imageButton7"
            android:gravity="center"
            android:layout_toRightOf="@+id/imageButton7"
            android:layout_alignTop="@+id/imageButton7"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="我的评论"
            android:textColor="@color/black"
            android:id="@+id/textView8"
            android:layout_alignBottom="@+id/imageButton8"
            android:gravity="center"
            android:layout_alignTop="@+id/imageButton8"
            android:layout_toRightOf="@+id/imageButton8" />

    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginTop="20dp"
        >

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:scaleType="centerInside"
            android:id="@+id/imageButton9"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:background="@color/white"
            android:src="@drawable/my_balance_icon"/>
        <ImageButton
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:scaleType="centerInside"
            android:id="@+id/imageButton10"
            android:background="@color/white"
            android:layout_marginTop="5dp"
            android:src="@drawable/icon_nuomi"
            android:layout_below="@+id/imageButton9"/>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="百度钱包"
            android:textColor="@color/black"
            android:id="@+id/textView69"
            android:layout_alignParentTop="true"
            android:layout_alignBottom="@+id/imageButton9"
            android:gravity="center"
            android:layout_toRightOf="@+id/imageButton9"
            android:layout_alignTop="@+id/imageButton9"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="百度糯米"
            android:textColor="@color/black"
            android:id="@+id/textView10"
            android:layout_alignBottom="@+id/imageButton10"
            android:gravity="center"
            android:layout_toRightOf="@+id/imageButton10"
            android:layout_alignTop="@+id/imageButton10"/>

    </RelativeLayout>

</LinearLayout>